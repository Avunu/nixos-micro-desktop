{
	"mainBar": {
		"layer": "top",
		"position": "bottom",
		"mod": "overlay",
		"exclusive": true,
		"gtk-layer-shell": false,
		"margin-bottom": -1,
		"passthrough": false,
		"height": 48,
		"modules-left": [
			"custom/os_button",
			"custom/terminal",
			"custom/thunar",
			"custom/browser",
			"custom/calculator",
			"wlr/taskbar"
		],
		"modules-center": [],
		"modules-right": [
			"network",
			"pulseaudio",
			"battery",
			"clock",
			"custom/swaync"
		],
		"network": {
			"format-wifi": "{icon}",
			"format-ethernet": "",
			"format-disconnected": "Û∞åô",
			"tooltip-format": "{icon} {essid}",
			"format-icons": [
				"Û∞§Ø",
				"Û∞§ü",
				"Û∞§¢",
				"Û∞§¢",
				"Û∞§®"
			],
			"on-click-right": "gnome-console -e nmtui"
		},
		"battery": {
			"states": {
				"good": 95,
				"warning": 30,
				"critical": 20
			},
			"format": "{icon}",
			"format-charging": " {capacity}%",
			"format-plugged": " {capacity}%",
			"format-alt": "{time} {icon}",
			"format-icons": [
				"Û∞Çé",
				"Û∞Å∫",
				"Û∞Åª",
				"Û∞Åº",
				"Û∞ÅΩ",
				"Û∞Åæ",
				"Û∞Åø",
				"Û∞ÇÄ",
				"Û∞ÇÅ",
				"Û∞ÇÇ",
				"Û∞Åπ"
			]
		},
		"pulseaudio": {
			"max-volume": 150,
			"scroll-step": 10,
			"format": "{icon}",
			"tooltip-format": "{icon}  {volume}%",
			"format-icons": {
				"format-muted": "",
				"default": [
					"",
					"",
					""
				]
			},
			"on-click": "gnome-control-center sound"
		},
		"custom/os_button": {
			"format": "‚äû",
			"on-click": "fuzzel",
			"tooltip": false
		},
		"clock": {
			"format": "      {:%R\n %d.%m.%Y}",
			"tooltip-format": "<tt><small>{calendar}</small></tt>",
			"calendar": {
				"mode": "year",
				"mode-mon-col": 3,
				"weeks-pos": "right",
				"on-scroll": 1,
				"on-click-right": "mode",
				"format": {
					"months": "<span color='#ffead3'><b>{}</b></span>",
					"days": "<span color='#ecc6d9'><b>{}</b></span>",
					"weeks": "<span color='#99ffdd'><b>W{}</b></span>",
					"weekdays": "<span color='#ffcc66'><b>{}</b></span>",
					"today": "<span color='#ff6699'><b><u>{}</u></b></span>"
				}
			},
			"actions": {
				"on-click-right": "mode",
				"on-click-forward": "tz_up",
				"on-click-backward": "tz_down",
				"on-scroll-up": "shift_up",
				"on-scroll-down": "shift_down"
			}
		},
		"wlr/taskbar": {
			"format": "{icon}",
			"icon-size": 40,
			"spacing": 3,
			"on-click-middle": "close",
			"tooltip-format": "{title}",
			"ignore-list": [],
			"on-click": "activate"
		},
		"custom/thunar": {
			"format": "üìÅ",
			"spacing": 3,
			"on-click-middle": "pkill nautilus",
			"tooltip-format": "Launch File Manager",
			"ignore-list": [],
			"on-click": "nautilus"
		},
		"custom/terminal": {
			"format": "‚¨õ",
			"spacing": 3,
			"on-click-middle": "pkill gnome-console",
			"tooltip-format": "Launch Terminal",
			"ignore-list": [],
			"on-click": "gnome-console"
		},
		"custom/swaync": {
			"tooltip": false,
			"format": "üîî",
			"on-click": "swaync-client -t -sw",
			"on-click-right": "swaync-client -d -sw"
		},
		"custom/browser": {
			"format": "üåê",
			"spacing": 3,
			"tooltip-format": "Launch Browser",
			"on-click": "firefox"
		},
		"custom/calculator": {
			"format": "üßÆ",
